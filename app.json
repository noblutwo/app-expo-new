  "orientation": "portrait",
  "icon": "./src/assets/images/icon.png",
  "userInterfaceStyle": "light",
  "splash": {
    "image": "./src/assets/images/splash.png",
    "resizeMode": "contain",
    "backgroundColor": "#ffffff"
  },
  "ios": {
    "bundleIdentifier": "com.noblutwo.vneid",
    "buildNumber": "1.0.0",
    "supportsTablet": true
  },
  "plugins": [
    [
      "expo-router"
    ],
    [
      "expo-build-properties",
      {
        "ios": {
          "useFrameworks": "static",
          "deploymentTarget": "15.1"
        }
      }
    ]
  ],
  "extra": {
    "eas": {
      "projectId": "cd8b3ba0-c36a-4e16-946d-d215a17c3240"
    },
    "router": {
      "origin": false
    }
  },
  "owner": "noblutwo",
  "sdkVersion": "52.0.0",
  "platforms": [
    "ios",
    "android"
  ]
}